			  ( 1354650774) (-1395561938) ( 318665647) (-18886906)
			  (-1361677255) ( 1390149678) (-317474532) ( 18728266)
			  (  447877887) ( -451382027) ( 103201434) ( -6046692)
			  (   56797556) (  -59779544) (  13810295) (  -848290)
